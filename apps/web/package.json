{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend Next.js (App Router) para Analizador de Complejidades",
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "build:ci": "NEXT_TELEMETRY_DISABLED=1 next build --no-lint",
    "start": "next start -p 3000",
    "test": "echo \"(tests se agregarán más adelante)\" && exit 0",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@aa/grammar": "workspace:^",
    "@aa/types": "workspace:^",
    "katex": "0.16.10",
    "lucide-react": "^0.544.0",
    "next": "14.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/katex": "^0.16.7",
    "@types/node": "20.14.9",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "10.4.20",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "postcss": "8.4.47",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.13",
    "typescript": "5.5.4"
  }
}
